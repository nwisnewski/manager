{
  "compilerOptions": {
    "types": [ "algoliasearch",
      "bluebird",
      "chart.js",
      "classnames",
      "enzyme",
      "hapi",
      "he",
      "inert",
      "invariant",
      "jest",
      "js-cookie",
      "jspdf",
      "md5",
      "memoizee",
      "moment-timezone",
      "node",
      "normalizr",
      "promise.prototype.finally",
      "qrcode.react",
      "qs",
      "ramda",
      "react-dom",
      "react-loadable",
      "react-redux",
      "react-router-dom",
      "react-select",
      "react-sticky",
      "recompose",
      "redux",
      "redux-thunk",
      "storybook__addon-actions",
      "storybook__addon-knobs",
      "storybook__react",
      "throttle-debounce",
      "url-parse",
      "uuid",
      "why-did-you-update",
      "xml2js",
      "yup",
      "zxcvbn" ],
    "outDir": "build/dist",
    "module": "esnext",
    "target": "es5",
    "lib": ["es2017", "dom"],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "rootDir": "src",
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "build",
    "e2e",
    "scripts",
    "acceptance-tests",
    "webpack",
    "jest",
    "src/setupTests.ts",
    "src/components/Storyshots.test.tsx",
    "src/*.test.*",
    "src/**/*.test.*",
    "src/**/**/*.test.*"
  ]
}
